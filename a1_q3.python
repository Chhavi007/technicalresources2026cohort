#Que3
t=int(input('How many numbers to be checked:'))
for i in range(0,t):
    n=int(input('Enter a number:'))
    x=[]
    for j in range(2,n):
        if n%j==0:
            x.append(j)
        else:
            continue
    if len(x)==0:
        print('Prime')
    else:
        print('Not Prime')
